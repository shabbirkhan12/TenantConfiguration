{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5003"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DBSettings": {
    "ConnectionString": "mongodb://<USERNAME>:<PASSWORD>@<DB_ADDRESS>:27017/?replicaSet=rs0&readPreference=secondaryPreferred&retryWrites=false",
    "DatabaseName": "<DB_NAME>"
  },
  "StorageSettings": {
    "stream_folder": "/mnt/data/streamcaches",
    "drawing_folder": "/mnt/data/drawings"
  },
  "AllowedHosts": "*"
}