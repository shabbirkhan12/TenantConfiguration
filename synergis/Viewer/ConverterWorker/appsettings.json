      "IDW": {
        "AllColorsAsBlack": true, // default: false
        "RemoveLineWeights": false // default: false
      }
    },
    "DXConverter": {
      "geom_vector_dpi": 1200, // Default value is 1200. For versions R2.2.3 and earlier, the default value was 4800.
      "geom_raster_dpi": 400 // Default value is 400
    },
    "DWGConverter": {
      "ltscale_custom_enabled": 1, // enable/disable custom LTSCALE adjustment
      "ltscale_custom_g": 1, // minimum gap size in a line pattern (on 4K display)
      "ltscale_custom_nmax": 100, // maximum linetype pattern count
      "ltscale_custom_pct": 5 // minimum percentage of lines exceeding nmax before adjustment applies
    }
  },
